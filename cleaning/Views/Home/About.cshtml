                                    <option value="50">Up To 900</option>
                                    <option value="60">Up To 1100</option>
                                    <option value="70">Up To 1300</option>
                                    <option value="80">Up To 1500</option>
                                    <option value="90">Up To 1800</option>
                                    <option value="100">Up To 2000</option>
                                    <option value="130">Up To 2400</option>
                                    <option value="140">More Than 2400</option>
                                </select>
                            </div>

                        </div>
                        <div class="col-md-4">

                            <!-- Name -->
                            <span class="txt9">
                                Name
                            </span>

                            <div class="wrap-inputname size12 bo2 bo-rad-10 m-t-3 m-b-23">
                                <input class="bo-rad-10 sizefull txt10 p-l-20" type="text" name="sname" placeholder="Name">
                            </div>

                            <!-- Phone -->
                            <span class="txt9">
                                Phone
                            </span>

                            <div class="wrap-inputphone size12 bo2 bo-rad-10 m-t-3 m-b-23">
                                <input class="bo-rad-10 sizefull txt10 p-l-20" type="text" name="sphone" placeholder="Phone">
                            </div>

                            <!-- People -->
                            <span class="txt9">
                                Number Of Bedrooms
                            </span>

                            <div class="form-group wrap-inputpeople size12 bo2 bo-rad-10 m-t-3 m-b-23">
                                <!-- Select2 -->
                                <select class="form-control" style="height:46px;" ng-model="a" name="Bedrooms">
                                    <option value="10">One Bedroom</option>
                                    <option value="20">Two Bedrooms</option>
                                    <option value="30">Three Bedrooms</option>
                                    <option value="40">Four Bedrooms</option>
                                    <option value="50">Five Bedrooms</option>
                                    <option value="60">Six Bedrooms</option>
                                    <option value="70">Seven Bedrooms</option>
                                    <option value="80">Eight Bedrooms</option>
                                    <option value="90">Nine Bedrooms</option>
                                    <option value="100">Ten Bedrooms</option>
                                    <option value="110">Eleven Bedrooms</option>
                                    <option value="120">Twelve Bedrooms</option>
                                </select>
                            </div>
                            <!-- People -->
                            <span class="txt9">
                                Comments
                            </span>

                            <div class="wrap-inputname size12 bo2 bo-rad-10 m-t-3 m-b-23">
                                <input class="bo-rad-10 sizefull txt10 p-l-20" type="text" name="name" placeholder="Comments">
                            </div>
                          
                        </div>
                    </div>
                        <div class="wrap-btn-booking flex-c-m m-t-30">
                            <h4 ng-show="a" class="txt5 m-b-18">
                                Amazing Price $ {{  a--c--d--e--f--g--h--b}}
                                </h4>
                        </div>
                        <div class="wrap-btn-booking flex-c-m m-t-50">
                            <!-- Button3 -->
                            <button type="submit" onclick="" class="btn3 flex-c-m size13 txt11 trans-0-4">
                                Reserve 
                            </button>
                        </div>
                </form>    
